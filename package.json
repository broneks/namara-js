{
  "name": "namara",
  "version": "0.1.0",
  "description": "The official Javascript client for the Namara open data service",
  "main": "lib/namara.js",
  "scripts": {
    "test": "./node_modules/.bin/jasmine && ./node_modules/karma/bin/karma start spec/support/karma.conf.js",
    "jasmine": "./node_modules/.bin/jasmine",
    "karma": "./node_modules/karma/bin/karma start spec/support/karma.conf.js",
    "compile": "./node_modules/.bin/webpack --config webpack.conf.js",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namara-io/namara-js.git"
  },
  "keywords": [
    "namara",
    "open data",
    "api",
    "browser",
    "client-side"
  ],
  "author": "ThinkData Works",
  "license": "Apache-2.0",
  "bugs": "https://github.com/namara-io/namara-js/issues",
  "homepage": "https://github.com/namara-io/namara-js",
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "jasmine": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-jasmine": "^0.3.8",
    "karma-webpack": "^1.7.0",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "es6-promise": "^3.1.2"
  }
